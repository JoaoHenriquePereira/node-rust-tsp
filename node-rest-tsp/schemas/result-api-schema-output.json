{
    "title": "result api output schema v1",
    "type": "object",
    "required": ["graph", "fitness","_links"],
    "properties": {
        "tour": {
            "type": "array",
            "uniqueItems": true,
            "items": {
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string"
                    },
                    "coordinates": {
                        "type": "array",
                        "items": {
                            "type": "number"
                        }
                    }
                }
            }
        },
        "distance": {
            "type": "number"
        },
        "fitness": {
            "type": "number"
        },
        "_links": {
            "type": "object"
        }
    }
}